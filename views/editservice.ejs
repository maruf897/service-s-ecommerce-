<h1>Edit Service</h1>
    <form id="serviceform"  >
        <label for="servicetitle">Service Title </label>
        <input type="text" name="title" id="title" value="<%= service.title %>">
        <label for="servicedescription">Service Description</label>
        <input type="text" name="description" id="description"value="<%= service.title %>">
        <br>
        <% service.companies.forEach((company,i)=>{ %>
        <label for="servicecompany1">Comapany Name </label>
        <input type="text" name="servicecompanyname<%= i+1 %>" id="servicecompanyname<%= i+1 %>"value="<%= company.name %>">
        <label for="servicecompany1">Price for suv </label>
        <input type="text" name="company1suvprice" id="company<%= i+1 %>suvprice"value="<%= company.suvprice %>">
        <label for="servicecompany1">Price for minivan </label>
        <input type="text" name="company1minivanprice" id="company<%= i+1 %>minivanprice"value="<%= company.minivanprice %>">
        <label for="servicecompany1">Price for sedan </label>
        <input type="text" name="company1sedanprice" id="company<%= i+1 %>sedanprice"value="<%= company.sedanprice %>">
        <br>
        <% }) %>
        <input type="text" id="noOfCompanies" name="noOfCompanies"  style="display: none;" value="<%= service.companies.length %>">
        <input type="text" id="link"  style="display: none;" value="/service/edit/<%= service._id%>">
        
    </form>
    
    <button id="addcompany">add more company</button>
    <button  id="submitBttn">Submit</button>

    <script
  src="https://code.jquery.com/jquery-3.5.1.min.js"
  integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
  crossorigin="anonymous"></script>
    <script src="/js/editingservice.js"></script>