    </div>
    <div class="searchedItems">

    </div>
    </div>
    <div class="service">
        <h4>Service List</h4>
        <% services.forEach((service)=>{ %>
        <a href="/service/<%= service._id %>"><%= service.title %></a>
   
        <br>
        <% }) %>
    </div>
